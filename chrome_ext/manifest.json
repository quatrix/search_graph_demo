{
  "name": "search_ext",
  "version": "1.0",
  "description": "search capture demo",
  "permissions": [
    "tabs",
    "webNavigation",
    "storage",
    "history",
    "<all_urls>"
  ],
  "background": {
    "page": "background.html"
  },
  "options_page": "options.html",

  "browser_action": {
    "default_icon": "online-icon.png",
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}
